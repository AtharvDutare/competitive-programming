{
    "files.associations": {
        "*.cpp": "cpp",
        "iostream": "cpp",
        "cmath": "cpp",
        "utility": "cpp",
        "algorithm": "cpp",
        "list": "cpp",
        "xstring": "cpp",
        "initializer_list": "cpp",
        "xutility": "cpp",
        "memory": "cpp",
        "atomic": "cpp",
        "ios": "cpp",
        "system_error": "cpp",
        "xiosbase": "cpp",
        "limits": "cpp",
        "istream": "cpp",
        "type_traits": "cpp",
